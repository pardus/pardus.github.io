#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules/tailwindcss/lib/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules/tailwindcss/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules/tailwindcss/lib/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules/tailwindcss/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/tailwindcss@3.3.1_postcss@8.4.21/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
